ues(responseData);\n\n  return (\n    <Layout>\n    <html>\n        <head>\n            <style>\n\n            </style>\n        </head>\n        <body>\n            <h1>Welcome to the Running Course Page of { dept_name } Department</h1>\n            <table id=\"departments-table\">\n                <thead>\n                    <tr>\n                        <th>S.No.</th>\n                        <th>Course Id</th>\n                        <th>Course Title</th>\n                    </tr>\n                </thead>\n\n                <tbody>\n                    {\n                    responseDataArray.map((item, index) => (\n                        <tr key={index}>\n                            <td>{index + 1}</td>\n                            <td><a href={`/course/${item.course_id}`}>{item.course_id}</a></td>\n                            <td>{item.title}</td>\n                        </tr>\n                    ))}\n                </tbody>\n            </table>\n        </body>\n    </html>\n    </Layout>\n  );\n}\n\nexport default DeptName;","/home/utkarsh/SEM6/CS387/200050145-200050146/cs387-assigments/lab4/IITASC/client/src/components/Table.js",[],[],"/home/utkarsh/SEM6/CS387/200050145-200050146/cs387-assigments/lab4/IITASC/client/src/components/Registration.js",["79","80"],[],"/home/utkarsh/SEM6/CS387/200050145-200050146/cs387-assigments/lab4/IITASC/client/src/components/Navbar.js",["81"],[],"/home/utkarsh/SEM6/CS387/200050145-200050146/cs387-assigments/lab4/IITASC/client/src/components/Layout.js",[],[],{"ruleId":"82","severity":1,"message":"83","line":1,"column":10,"nodeType":"84","messageId":"85","endLine":1,"endColumn":23},{"ruleId":"82","severity":1,"message":"86","line":1,"column":38,"nodeType":"84","messageId":"85","endLine":1,"endColumn":46},{"ruleId":"82","severity":1,"message":"87","line":1,"column":47,"nodeType":"84","messageId":"85","endLine":1,"endColumn":53},{"ruleId":"88","severity":1,"message":"89","line":22,"column":6,"nodeType":"90","endLine":22,